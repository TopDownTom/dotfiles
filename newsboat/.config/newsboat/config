# Set defaults
auto-reload yes
player mpv
browser "firefox --profile $HOME/.mozilla/firefox/managed --private-window %u"

# Keybinds
unbind-key j
unbind-key k
unbind-key q
unbind-key l

bind-key H	toggle-show-read-feeds
bind-key h	quit
bind-key j	down
bind-key k	up
bind-key l	open
bind-key U	show-urls

macro m set browser "tsp /usr/local/sbin/vidQueue %u"; open-in-browser

# Colors
highlight feedlist ".*(0/0))" black
